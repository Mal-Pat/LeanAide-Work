def SeqConvergesTo (x : ℕ → ℝ) (L : ℝ) : Prop :=
  ∀ ε > 0, ∃ N : ℕ, ∀ n > N, |x n - L| < ε
def BoundedSequence : (ℕ → ℝ) → Prop := fun x => ∃ M > 0, ∀ n : ℕ, |x n| ≤ M
abbrev ConvergentSequenceRealIsBounded.prop : Prop :=
  ∀ {u : ℕ → ℝ} {l : ℝ}, Filter.Tendsto u Filter.atTop (nhds l) → ∃ R > 0, ∀ (n : ℕ), |u n| ≤ R + |l|
