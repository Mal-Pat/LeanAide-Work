{
  "document": [
    {
      "type": "Theorem",
      "header": "Theorem",
      "label": "thm:sum_even_odd",
      "hypothesis": [
        {
          "type": "assume_statement",
          "assumption": "a is even"
        },
        {
          "type": "assume_statement",
          "assumption": "b is odd"
        }
      ],
      "claim": "The sum of an even number a and an odd number b is an odd number.",
      "proof": {
        "type": "Proof",
        "claim_label": "thm:sum_even_odd",
        "proof_steps": [
          [
            {
              "type": "some_statement",
              "variable_name": "k",
              "variable_kind": "integer",
              "statement": "There exists an integer k such that a = 2*k."
            },
            {
              "type": "some_statement",
              "variable_name": "m",
              "variable_kind": "integer",
              "statement": "There exists an integer m such that b = 2*m + 1."
            },
            {
              "type": "calculation",
              "inline_calculation": "a + b = (2*k) + (2*m + 1) = 2*(k + m) + 1"
            },
            {
              "type": "assert_statement",
              "claim": "2*(k + m) + 1 is odd."
            },
            {
              "type": "conclude_statement",
              "claim": "a + b is odd."
            }
          ]
        ]
      }
    }
  ]
}