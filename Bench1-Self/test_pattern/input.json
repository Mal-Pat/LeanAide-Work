{
  "document": [
    {
      "type": "Theorem",
      "label": "thm:remainder6",
      "header": "Theorem",
      "claim": "If the remainder of dividing a natural number n by 6 is 0 or 4, then n is not prime.",
      "proof": {
        "type": "Proof",
        "claim_label": "thm:remainder6",
        "proof_steps": [
          [
            {
              "type": "pattern_cases_statement",
              "on": "n % 6",
              "proof_cases": [
                {
                  "type": "pattern_case",
                  "pattern": "0",
                  "proof": {
                    "type": "Proof",
                    "claim_label": "thm:remainder6",
                    "proof_steps": [
                      [
                        {
                          "type": "some_statement",
                          "variable_name": "k",
                          "variable_kind": "natural number",
                          "statement": "n = 6 * k"
                        },
                        {
                          "type": "assert_statement",
                          "claim": "6 is a factor of n"
                        },
                        {
                          "type": "assert_statement",
                          "claim": "n is not prime"
                        }
                      ]
                    ]
                  }
                },
                {
                  "type": "pattern_case",
                  "pattern": "4",
                  "proof": {
                    "type": "Proof",
                    "claim_label": "thm:remainder6",
                    "proof_steps": [
                      [
                        {
                          "type": "some_statement",
                          "variable_name": "k",
                          "variable_kind": "natural number",
                          "statement": "n = 6 * k + 4"
                        },
                        {
                          "type": "calculation",
                          "inline_calculation": "n = 6 * k + 4 = 2 * (3 * k + 2)"
                        },
                        {
                          "type": "assert_statement",
                          "claim": "n is an even number greater than 2"
                        },
                        {
                          "type": "assert_statement",
                          "claim": "n is not prime"
                        }
                      ]
                    ]
                  }
                }
              ]
            }
          ]
        ]
      }
    }
  ]
}