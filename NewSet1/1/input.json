{
  "document": [
    {
      "type": "Definition",
      "label": "def:even",
      "header": "Definition",
      "definition": "An integer x is even if and only if there exists an integer k such that x = 2k."
    },
    {
      "type": "Definition",
      "label": "def:odd",
      "header": "Definition",
      "definition": "An integer x is odd if and only if there exists an integer ℓ such that x = 2ℓ + 1."
    },
    {
      "type": "Theorem",
      "label": "thm:even_plus_odd",
      "header": "Theorem",
      "claim": "For all integers m and n, if m is even and n is odd, then m + n is odd.",
      "proof": {
        "type": "Proof",
        "claim_label": "thm:even_plus_odd",
        "proof_steps": [
          [
            {
              "type": "let_statement",
              "variable_name": "m",
              "variable_type": "integer",
              "properties": "m is even",
              "statement": "m = 2k for some integer k"
            },
            {
              "type": "let_statement",
              "variable_name": "n",
              "variable_type": "integer",
              "properties": "n is odd",
              "statement": "n = 2ℓ + 1 for some integer ℓ"
            },
            {
              "type": "let_statement",
              "variable_name": "s",
              "value": "m + n",
              "statement": "Define s := m + n"
            },
            {
              "calculation_sequence": [
                "s = m + n",
                "= 2k + (2ℓ + 1)",
                "= 2(k + ℓ) + 1"
              ]
            },
            {
              "type": "let_statement",
              "variable_name": "p",
              "value": "k + ℓ",
              "variable_type": "integer",
              "statement": "Define p := k + ℓ"
            },
            {
              "type": "assert_statement",
              "claim": "s = 2p + 1 for some integer p"
            },
            {
              "type": "conclude_statement",
              "claim": "s is odd"
            }
          ]
        ]
      }
    }
  ]
}