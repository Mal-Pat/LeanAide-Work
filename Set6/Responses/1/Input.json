  "document": [
    {
      "type": "Theorem",
      "header": "Theorem",
      "label": "thm:even-odd-sum-odd",
      "claim": "The sum of an even natural number and an odd natural number is an odd natural number.",
      "proof": {
        "type": "Proof",
        "claim_label": "thm:even-odd-sum-odd",
        "proof_steps": [
          {
            "type": "LogicalStepSequence",
            "items": [
              {
                "type": "let_statement",
                "variable_name": "E",
                "variable_type": "even natural number",
                "statement": "Let the even natural number be E."
              },
              {
                "type": "let_statement",
                "variable_name": "O",
                "variable_type": "odd natural number",
                "statement": "Let the odd natural number be O."
              },
              {
                "type": "some_statement",
                "variable_name": "p",
                "variable_kind": "natural number",
                "properties": "such that E = 2*p",
                "statement": "Then there exists a natural number p such that E = 2*p."
              },
              {
                "type": "some_statement",
                "variable_name": "q",
                "variable_kind": "natural number",
                "properties": "such that O = 2*q + 1",
                "statement": "And there exists a natural number q such that O = 2*q + 1."
              },
              {
                "type": "let_statement",
                "variable_name": "S",
                "value": "E + O",
                "statement": "Take their sum to be S = E + O."
              },
              {
                "type": "assert_statement",
                "claim": "S = (2*p) + (2*q + 1)",
                "proof_method": "Substitution of E and O",
                "calculation": {
                  "inline_calculation": "S = (2*p) + (2*q + 1)"
                }
              },
              {
                "type": "assert_statement",
                "claim": "S = 2*(p + q) + 1",
                "proof_method": "Simplifying the expression",
                "calculation": {
                  "inline_calculation": "S = 2*(p + q) + 1"
                }
              },
              {
                "type": "let_statement",
                "variable_name": "k",
                "value": "p + q",
                "variable_type": "natural number",
                "properties": "since the sum of two natural numbers is another natural number",
                "statement": "Let p + q = k, since the sum of two natural numbers is another natural number."
              },
              {
                "type": "assert_statement",
                "claim": "S = 2*k + 1",
                "proof_method": "Substitution of k",
                "calculation": {
                  "inline_calculation": "S = 2*k + 1"
                }
              },
              {
                "type": "conclude_statement",
                "claim": "Since S = E + O can be expressed as 2*k + 1 for some natural number k, it is an odd number. Therefore, the sum of an even natural number and an odd natural number is an odd natural number."
              }
            ]
          }
        ]
      }
    }
  ]
